{
  "compilerOptions": {
    "target": "ES2020",
    "module": "none",
    "rootDir": "ts",
    "outDir": "js",
    "lib": ["DOM", "ES2020"],
    "types": [],
    "removeComments": false,
    "strict": false,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "sourceMap": false,
    "downlevelIteration": true
  },
  "include": ["ts/**/*.ts"],
  "exclude": ["js/**", "node_modules"]
}
